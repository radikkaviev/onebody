if @success
  if params[:super_admin]
    page.redirect_to administration_admins_path
  else
    @privs.each do |priv|
      page.visual_effect :highlight, "admin_#{params[:id]}_#{priv}"
      page << "$('#{priv}_#{params[:id]}').checked = #{params[:value]};"
    end
  end
else
  page.alert(t('admin.error_saving'))
end

