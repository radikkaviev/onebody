<% content_for :subnav do %>
  <%= link_to 'Events', :controller => 'events' %>
  <%= link_to 'Pictures', :controller => 'pictures' %>
  <%= link_to 'Verses', :controller => 'verses' %>
  <%= link_to 'Recipes', :controller => 'recipes' %>
  <% if Setting.get(:url, :news) %><%= link_to 'News', Setting.get(:url, :news) %><% end %>
  <% if @logged_in.admin?(:manage_music) or @logged_in.music_access? %>
    <%= link_to 'Music', :controller => 'music' %>
  <% end %>
<% end %>
